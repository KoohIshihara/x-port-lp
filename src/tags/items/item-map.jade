    :scope {
      display: block;
      .gmap {
        position: relative;
        padding-bottom: 56.25%;
        padding-top: 30px;
        height: 0;
        overflow: hidden;
      }
      .gmap iframe {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
      }
      @media only screen and (max-width : 600px) {
        .gmap iframe {
          width: 100vw;
        }
      }
    }
  
  
